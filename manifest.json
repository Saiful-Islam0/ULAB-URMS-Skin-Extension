{
  "manifest_version": 3,
  "name": "URMS Skin Redesign",
  "version": "1.0",
  "description": "A modern skin overlay for the URMS website at ULAB.",
  "content_scripts": [
    {
      "matches": ["https://urms-online.ulab.edu.bd/*"],
      "js": ["content.js"],
      "css": ["custom.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "host_permissions": ["https://urms-online.ulab.edu.bd/*"],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,"
  }
}
